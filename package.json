    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "highlight.js": "^9.12.0",
    "marked": "^0.3.17",
    "nuxt": "^1.4.0",
    "vue-markdown": "^2.2.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
