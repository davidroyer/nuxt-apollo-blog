{
  "name": "nuxt-apollo-blog",
  "version": "1.0.0",
  "description": "GraphCMS Blog starter using Apollo Client and Nuxt.js",
  "author": "hmeissner <hugomeissner@icloud.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt -p 3002",
    "build": "nuxt build",
